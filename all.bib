@INPROCEEDINGS{2001:troffaes::ipdisc,
  AUTHOR = {Matthias C. M. Troffaes and Gert {de Cooman} and Dirk Aeyels},
  TITLE = {Imprecise probabilities---discussion and open problems},
  BOOKTITLE = {Proceedings of the 2nd FTW PhD Symposium},
  YEAR = 2001,
  MONTH = {Dec},
  ABSTRACT = {If only little, vague or conflicting information is 
  available about a system, it is hard to construct and motivate a
  model based on the so-called precise probability theory.  In this
  case, generalisations of this theory---called in the literature
  imprecise probability theories---prove to be useful. We discuss the
  most satisfactory of these models, the unifying behavioural theory
  of lower previsions, we identify a number of open problems in the
  theory, and we briefly point to work that is being done to solve
  them.},
  url = {preprint/2001troffaes+decooman+aeyels_improb_abstract.pdf}
}

